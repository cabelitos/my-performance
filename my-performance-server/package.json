{
   "name": "my-performance-server",
   "version": "0.0.1",
   "private": true,
   "description": "My performance server",
   "license": "MIT",
   "devDependencies": {
      "@types/node": "^12.12.0",
      "@typescript-eslint/eslint-plugin": "^2.6.0",
      "@typescript-eslint/parser": "^2.6.0",
      "eslint": "5.16.0",
      "eslint-config-airbnb-base": "^14.0.0",
      "eslint-config-prettier": "^6.5.0",
      "eslint-plugin-import": "^2.18.2",
      "eslint-plugin-prettier": "^3.1.1",
      "nodemon": "^1.19.4",
      "npm-run-all": "^4.1.5",
      "prettier": "^1.18.2",
      "prettier-eslint": "^9.0.0",
      "ts-node": "8.4.1",
      "typeorm-fixtures-cli": "^1.3.7",
      "typescript": "3.6.4"
   },
   "dependencies": {
      "apollo-server": "^2.9.7",
      "cross-env": "^6.0.3",
      "dotenv-flow": "^3.1.0",
      "graphql": "^14.5.8",
      "pg": "^7.12.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.20"
   },
   "scripts": {
      "start": "cross-env NODE_ENV=development nodemon",
      "build": "cross-env NODE_ENV=production tsc -p tsconfig.json",
      "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}'",
      "docker:build": "docker-compose build",
      "docker:up": "cross-env NODE_ENV=production docker-compose up",
      "fixtures": "node -r dotenv-flow/config ./node_modules/.bin/fixtures ./src/fixtures --config ormconfig.json --sync",
      "run-fixtures": "run-s build fixtures"
   }
}
